import java.util.Arrays;

public class selection {
    public static void main(String[] args) {
        int arr[]={2,9,3,1,10};
    
        for(int i=0;i<arr.length-1;i++){
            int min=i;
            for(int j=i;j<arr.length;j++){
                if(arr[j]<arr[min]){
                   min=j;
                }
            }
            int temp=arr[min];
            arr[min]=arr[i];
            arr[i]=temp;

        }
        System.out.print(Arrays.toString(arr));
    }
    
}
