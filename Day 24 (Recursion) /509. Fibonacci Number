class Solution{
    public int fib(int n){
        int x=0;
        int y=1;
        
        
        if(n<2){
            return n;
        }
        for(int i=2;i<=n;i++){
            int temp=x;
            x=y;
            y+=temp;
        }
        return y;
    }
}
