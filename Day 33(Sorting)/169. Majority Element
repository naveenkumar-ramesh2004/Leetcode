//169. Majority Element
//https://leetcode.com/problems/majority-element/?envType=problem-list-v2&envId=sorting

class Solution {
    public int majorityElement(int[] nums) {
        Arrays.sort(nums);
        int count=0;
        int max=-1;
        int index=0;
        for(int i=0;i<nums.length-1;i++){
            if(nums[i]==nums[i+1]){
                count++;
                if(count>max){
                    max=count;
                    index=i;
                }
            }
            else{
                count=0;
            }
        }
        return nums[index];
    }
}
